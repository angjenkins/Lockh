package gov.gsa.fas.AutoChoice.DTO;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import org.springframework.stereotype.Component;

/**
 * @author Arnel Macatula
 *
 */
@Entity
@Table (name="states")
@Component
public class StatesDTO {
	@Id
	@Column(name="State")
	protected String state = "";
	@Column(name="State_Name")
	protected String stateName = "";
	@Column(name="State_Capital")
	protected String stateCapital = "";
	@Column(name="Country")
	protected String country = "";
	@Column(name="User_Updated")
	protected String userUpdated = "";
	@Column(name="State_Code")
	protected String stateCode = "";
	@Column(name="FIPS_Code")
	protected String fIPSCode = "";
	@Column(name="Region")
	protected String region = "";
	@Column(name="USA_Cont")
	protected String usaCont = "";
	public String getState() {
		return state;
	}
	public void setState(String state) {
		this.state = state;
	}
	public String getStateName() {
		return stateName;
	}
	public void setStateName(String stateName) {
		this.stateName = stateName;
	}
	public String getStateCapital() {
		return stateCapital;
	}
	public void setStateCapital(String stateCapital) {
		this.stateCapital = stateCapital;
	}
	public String getCountry() {
		return country;
	}
	public void setCountry(String country) {
		this.country = country;
	}
	public String getUserUpdated() {
		return userUpdated;
	}
	public void setUserUpdated(String userUpdated) {
		this.userUpdated = userUpdated;
	}
	public String getStateCode() {
		return stateCode;
	}
	public void setStateCode(String stateCode) {
		this.stateCode = stateCode;
	}
	public String getfIPSCode() {
		return fIPSCode;
	}
	public void setfIPSCode(String fIPSCode) {
		this.fIPSCode = fIPSCode;
	}
	public String getRegion() {
		return region;
	}
	public void setRegion(String region) {
		this.region = region;
	}
	public String getUsaCont() {
		return usaCont;
	}
	public void setUsaCont(String usaCont) {
		this.usaCont = usaCont;
	}
	
}